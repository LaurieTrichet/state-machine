{
    "name": "state-machine",
    "version": "1.0.0",
    "description": "A implementation of the state pattern",
    "main": "main.js",
    "scripts": {
        "test": "sh run-test.sh",
        "test-with-coverage": "sh run-test.sh -c",
        "build": "npm run clean && webpack",
        "run": "node dist/main.js",
        "clean": "rm -rf dist/",
        "clean-test": "rm -rf test/dist/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LaurieTrichet/state-machine.git"
    },
    "keywords": [
        "state",
        "pattern"
    ],
    "author": "Laurie Trichet",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LaurieTrichet/state-machine/issues"
    },
    "homepage": "https://github.com/LaurieTrichet/state-machine#readme",
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-typescript": "^7.3.3",
        "@istanbuljs/nyc-config-typescript": "^0.1.3",
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.7",
        "babel-loader": "^8.3.0",
        "chai": "^4.2.0",
        "copy-webpack-plugin": "^11.0.0",
        "file-loader": "^6.2.0",
        "mocha": "^6.2.0",
        "nyc": "^14.1.1",
        "source-map-support": "^0.5.12",
        "ts-node": "^8.3.0",
        "typescript": "^3.5.3",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
    "nyc": {
        "include": [
            "src/**/*.ts",
            "src/**/*.tsx"
        ],
        "extension": [
            ".ts",
            ".tsx"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "lcov",
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true
    },
    "dependencies": {}
}